{
    "global": {
        "abstract": true,
        "type": "multiple_line",
        "size": {
            "width": 760,
            "height": 500
        },
        "seconds": 100
    },

    "battery-line": {
        "parent": "global",
        "event_names": ["battery"],
        "anchor": "#battery-anchor",
        "name_lines": ["Battery"],
        "y_name": "Battery",
        "accessors": [{
            "type": "primitive"
        }],

        "range": [0, 100]
    },

    "distances": {
        "abstract": true,
        "parent": "global",
        "event_names": ["distances"],
        "range": [0, 4]
    },
    "distances-center": {
        "parent": "distances",
        "name_lines": ["Front-left", "Front-center", "Front-right"],
        "y_name": "Distances front",
        "anchor": "#distances-center-anchor",
        "accessors": [{
            "type": "index",
            "start": 0,
            "end": 3
        }]
    },
    "distances-middle": {
        "parent": "distances",
        "name_lines": ["Middle-left", "Middle-right"],
        "y_name": "Distances middle",
        "anchor": "#distances-middle-anchor",
        "accessors": [{
            "type": "index",
            "start": 3,
            "end": 5
        }]
    },
    "distances-back": {
        "parent": "distances",
        "name_lines": ["Back-left", "Back-center", "Back-right"],
        "y_name": "Distances back",
        "anchor": "#distances-back-anchor",
        "accessors": [{
            "type": "index",
            "start": 5,
            "end": 8
        }]
    },

    "acceleration": {
        "parent": "global",
        "anchor": "#debug-anchor",
        "event_names": ["acceleration"],
        "name_lines": ["x", "y", "z"],
        "y_name": "Acceleration",
        "range": [0, 40],
        "accessors": [
                      {"type": "dict", "key": "x"},
                      {"type": "dict", "key": "y"},
                      {"type": "dict", "key": "z"}
                     ]
    },

    "axis-x": {
        "parent": "global",
        "anchor": "#axis-x-anchor",
        "event_names": ["acceleration", "rotation"],
        "name_lines": ["x-acceleration", "x-rotation"],
        "y_name": "Acceleration and rotation for x axis",
        "range": [0, 360],
        "accessors": [
                        {"type": "dict", "key": "x", "event": "acceleration"},
                        {"type": "index", "start": 0, "end": 1, "event": "rotation"}
                     ]
    },

    "motor": {
        "abstract": true,
        "parent": "global",
        "event_names": ["motor"]
    },
    "speed": {
        "parent": "motor",
        "name_lines": ["speed-left", "speed-right"],
        "y_name": "Speed",
        "anchor": "#speed-anchor",
        "accessors": [{
            "type": "index",
            "start": 0,
            "end": 2,
            "after": [{
                "type": "dict",
                "key": "speed"
            }]
        }],

        "range": [0, 5]
    }
}
