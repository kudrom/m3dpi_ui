{
    "global-general":{
        "abstract": "true",
        "size": {
            "width": 150
        },
        "event_names": ["date"]
    },

    "led": {
        "parent": "global-general",
        "type": "led",
        "event_names": ["leds"],
        "anchor": "#led-anchor",
        "accessors": [{
            "type": "index",
            "start": 0,
            "end": 1
        }],
        "radius": 60,
        "mapping": {
            "on": "green",
            "off": "red",
            "null": "grey"
        }
    },

    "battery-led": {
        "parent": "global-general",
        "type": "led",
        "event_names": ["battery"],
        "anchor": "#battery-led-anchor",
        "accessors": [{
            "type": "primitive"
        }],
        "radius": 25,
        "mapping": {
            "0 to 25": "red",
            "25 to 75": "yellow",
            "75 to 100": "green"
        }
    }
}
