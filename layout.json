{
    "global-leds": {
        "abstract": true,
        "size": {"height": 40},
        "event_names": ["distance"],
        "type": "led",
        "radius": 18,
        "mapping": {"0 to 20": "red", "20 to 100": "orange",
                  "100 to 200": "yellow", "200 to 255": "green"}
    },
    "led1": {
        "parent": "global-leds",
        "anchor": "#led1",
        "accessors": [{"type": "index", "start": 0, "end": 1}]
    },
    "led2": {
        "parent": "global-leds",
        "anchor": "#led2",
        "accessors": [{"type": "index", "start": 1, "end": 2}]
    },
    "led3": {
        "parent": "global-leds",
        "anchor": "#led3",
        "accessors": [{"type": "index", "start": 2, "end": 3}]
    },
    "led4": {
        "parent": "global-leds",
        "anchor": "#led4",
        "accessors": [{"type": "index", "start": 3, "end": 4}]
    },
    "led5": {
        "parent": "global-leds",
        "anchor": "#led5",
        "accessors": [{"type": "index", "start": 4, "end": 5}]
    },
    "led6": {
        "parent": "global-leds",
        "anchor": "#led6",
        "accessors": [{"type": "index", "start": 5, "end": 6}]
    },
    "led7": {
        "parent": "global-leds",
        "anchor": "#led7",
        "accessors": [{"type": "index", "start": 6, "end": 7}]
    },
    "led8": {
        "parent": "global-leds",
        "anchor": "#led8",
        "accessors": [{"type": "index", "start": 7, "end": 8}]
    },

    "global-general":{
        "abstract": "true",
        "size": {
            "height": 60
        },
        "event_names": ["time"],
        "accessors": [{"type": "primitive"}]
    },

    "device-time": {
        "parent": "global-general",
        "type": "digital_clock",
        "device_time": true,
        "anchor": "#device-time-anchor"
    },

    "date": {
        "parent": "global-general",
        "type": "digital_date",
        "anchor": "#date-anchor",
        "format": "DD/MM/YYYY"
    },

    "real-time": {
        "parent": "global-general",
        "type": "digital_clock",
        "device_time": false,
        "anchor": "#real-time-anchor"
    },


    "global-lines": {
        "abstract": true,
        "type": "multiple_line",
        "size": {
            "width": 600,
            "height": 400
        },
        "seconds": 100
    },

    "distances": {
        "abstract": true,
        "parent": "global-lines",
        "event_names": ["distance"],
        "range": [0, 255]
    },
    "distances-center": {
        "parent": "distances",
        "name_lines": ["Front", "Front-right", "Front-left"],
        "y_name": "Distances front",
        "anchor": "#distances-front-anchor",
        "accessors": [
            {
                "type": "index",
                "start": 0,
                "end": 2
            },
            {
                "type": "index",
                "start": 7,
                "end": 8
            }
        ]
    },
    "distances-middle": {
        "parent": "distances",
        "name_lines": ["Right", "Left"],
        "y_name": "Distances center",
        "anchor": "#distances-center-anchor",
        "accessors": [
            {
                "type": "index",
                "start": 2,
                "end": 3
            },
            {
                "type": "index",
                "start": 6,
                "end": 7
            }
        ]
    },
    "distances-back": {
        "parent": "distances",
        "name_lines": ["Back-right", "Back", "Back-left"],
        "y_name": "Distances back",
        "anchor": "#distances-back-anchor",
        "accessors": [{
            "type": "index",
            "start": 3,
            "end": 6
        }]
    },

    "acceleration": {
        "parent": "global-lines",
        "anchor": "#acceleration-anchor",
        "event_names": ["acceleration"],
        "name_lines": ["x", "y", "z"],
        "y_name": "Acceleration",
        "range": [0, 20000],
        "accessors": [
                      {"type": "dict", "key": "x"},
                      {"type": "dict", "key": "y"},
                      {"type": "dict", "key": "z"}
                     ]
    },

    "direction": {
        "parent": "global-lines",
        "anchor": "#direction-anchor",
        "event_names": ["direction"],
        "name_lines": ["x", "y", "z"],
        "y_name": "Direction",
        "range": [0, 40],
        "accessors": [
                      {"type": "dict", "key": "x"},
                      {"type": "dict", "key": "y"},
                      {"type": "dict", "key": "z"}
                     ]
    },

    "rotation": {
        "parent": "global-lines",
        "anchor": "#rotation-anchor",
        "event_names": ["rotation"],
        "name_lines": ["x", "y", "z"],
        "y_name": "Rotation",
        "range": [0, 40],
        "accessors": [
                      {"type": "dict", "key": "x"},
                      {"type": "dict", "key": "y"},
                      {"type": "dict", "key": "z"}
                     ]
    }
}
